<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs">

	<class name="org.openmrs.formentry.FormEntryQueue" table="formentry_queue">

		<id name="formEntryQueueId" type="int" column="formentry_queue_id" 
			unsaved-value="0">
				<generator class="native" />
		</id>

		<property name="formData" type="java.lang.String" 
			column="form_data" not-null="true" length="16777215" />
			
		<property name="status" type="int" column="status" not-null="true" />
		
		<property name="dateProcessed" type="java.util.Date"
			column="date_processed" />
		
		<property name="errorMsg" type="java.lang.String"
			column="error_msg" />

		<many-to-one name="creator" class="User" not-null="true">
			<column name="creator" />
		</many-to-one>

		<property name="dateCreated" type="java.util.Date" 
			column="date_created" not-null="true" length="19" />

	</class>
</hibernate-mapping>
