<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class name="org.openmrs.ConceptNumeric" table="concept_numeric">

	
	<!--
	<composite-id>
			<key-many-to-one name="concept" column="concept_id"
				class="org.openmrs.Concept" />
	</composite-id>
	-->
	<id name="conceptId" column="concept_id" unsaved-value="null">
		<generator class="foreign" >
        	<param name="property">concept</param>
        </generator>
    </id>
	
    <property
        name="hiAbsolute"
        type="java.lang.Double"
        column="hi_absolute"
        length="22"
    />
    <property
        name="hiCritical"
        type="java.lang.Double"
        column="hi_critical"
        length="22"
    />
    <property
        name="hiNormal"
        type="java.lang.Double"
        column="hi_normal"
        length="22"
    />
    <property
        name="lowAbsolute"
        type="java.lang.Double"
        column="low_absolute"
        length="22"
    />
    <property
        name="lowCritical"
        type="java.lang.Double"
        column="low_critical"
        length="22"
    />
    <property
        name="lowNormal"
        type="java.lang.Double"
        column="low_normal"
        length="22"
    />
    <property
        name="units"
        type="java.lang.String"
        column="units"
        length="50"
    />
    <property
        name="dateCreated"
        type="java.util.Date"
        column="date_created"
        not-null="true"
        length="19"
    />
    <property
        name="precise"
        type="java.lang.Boolean"
        column="precise"
        length="1"
    />
    <property
        name="dateChanged"
        type="java.util.Date"
        column="date_changed"
        length="19"
    />

    <!-- Associations -->
  
    <!-- bi-directional one-to-one association to Concept -->
    <one-to-one
        name="concept"
        class="org.openmrs.Concept"
        constrained="true"
    />
    <!-- bi-directional many-to-one association to User -->
    <many-to-one
        name="creator"
        class="org.openmrs.User"
        not-null="true"
        column="creator"/>
    <!-- bi-directional many-to-one association to User -->
    <many-to-one
        name="changedBy"
        class="org.openmrs.User"
        not-null="true"
        column="changed_by"/>

</class>
</hibernate-mapping>
