<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs">

	<class name="Obs" table="obs">

		<id name="obsId" type="int" column="obs_id">
			<generator class="assigned" />
		</id>

		<property name="obsDatetime" type="java.util.Date"
			column="obs_datetime" not-null="true" length="19" />
		<property name="obsGroupId" type="java.lang.Integer"
			column="obs_group_id" length="11" />
		<property name="valueGroupId" type="java.lang.Integer"
			column="value_group_id" length="11" />
		<property name="valueBoolean" type="boolean"
			column="value_boolean" length="1" />
		<property name="valueDatetime" type="java.util.Date"
			column="value_datetime" length="19" />
		<property name="valueNumeric" type="double"
			column="value_numeric" length="22" />
		<property name="valueModifier" type="java.lang.String"
			column="value_modifier" length="2" />
		<property name="valueText" type="java.lang.String"
			column="value_text" length="50" />
		<property name="comment" type="java.lang.String"
			column="comment" length="255" />
		<property name="dateCreated" type="java.util.Date"
			column="date_created" not-null="true" length="19" />
		<property name="voided" type="java.lang.Boolean" column="voided"
			length="1" />
		<property name="dateVoided" type="java.util.Date"
			column="date_voided" length="19" />
		<property name="voidReason" type="java.lang.String"
			column="void_reason" length="255" />

		<!-- Associations -->

		<!-- bi-directional many-to-one association to Patient -->
		<many-to-one name="patient" class="Patient" not-null="true">
			<column name="patient_id" />
		</many-to-one>
		<!-- bi-directional many-to-one association to Concept -->
		<many-to-one name="concept" class="Concept"
			not-null="true">
			<column name="concept_id" />
		</many-to-one>
		<!-- bi-directional many-to-one association to Concept -->
		<many-to-one name="valueCoded" class="Concept"
			not-null="true">
			<column name="value_coded" />
		</many-to-one>
		<!-- bi-directional many-to-one association to Order -->
		<many-to-one name="order" class="Order" not-null="true">
			<column name="order_id" />
		</many-to-one>
		<!-- bi-directional many-to-one association to Location -->
		<many-to-one name="location" class="Location" not-null="true">
			<column name="location_id" />
		</many-to-one>
		<!-- bi-directional one-to-one association to ComplexObs -->
		<one-to-one name="complexObs" class="ComplexObs"
			outer-join="auto" />
		<!-- bi-directional many-to-one association to Encounter -->
		<many-to-one name="encounter" class="Encounter"
			not-null="true">
			<column name="encounter_id" />
		</many-to-one>
		<!-- bi-directional many-to-one association to User -->
		<many-to-one name="creator" class="User"
			not-null="true">
			<column name="creator" />
		</many-to-one>
		<!-- bi-directional many-to-one association to User -->
		<many-to-one name="voidedBy" class="User"
			not-null="true">
			<column name="voided_by" />
		</many-to-one>

	</class>
	
</hibernate-mapping>
