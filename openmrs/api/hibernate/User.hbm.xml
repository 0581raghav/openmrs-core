		<!-- bi-directional many-to-many association to Role -->
		<set name="roles" table="user_role" lazy="true" cascade="none">
			<key>
				<column name="user_id" />
			</key>
			<many-to-many class="Role">
				<column name="role" />
			</many-to-many>
		</set>

