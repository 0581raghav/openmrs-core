<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs">

	<class name="User" table="users">

		<id name="userId" type="int" column="user_id"
			unsaved-value="0">
			<generator class="identity" />
		</id>

		<property name="username" type="java.lang.String"
			column="username" length="50" />
		<property name="firstName" type="java.lang.String"
			column="first_name" length="50" />
		<property name="middleName" type="java.lang.String"
			column="middle_name" length="255" />
		<property name="lastName" type="java.lang.String"
			column="last_name" length="50" />
		<property name="password" type="java.lang.String"
			column="password" length="50" insert="false" update="false" />
		<property name="dateCreated" type="java.util.Date"
			column="date_created" not-null="true" length="19" />
		<property name="dateChanged" type="java.util.Date"
			column="date_changed" length="19" />
		<property name="voided" type="java.lang.Boolean" column="voided"
			length="1" />
		<property name="dateVoided" type="java.util.Date"
			column="date_voided" length="19" />

