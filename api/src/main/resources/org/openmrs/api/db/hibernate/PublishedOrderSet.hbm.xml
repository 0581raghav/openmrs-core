<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs">

	<class name="PublishedOrderSet" table="published_order_set">
		
		<id name="publishedOrderSetId" type="int" column="published_order_set_id" unsaved-value="null">
			<generator class="native" />
		</id>
		
		<many-to-one name="concept" class="Concept" not-null="true" column="concept_id" />
		
		<many-to-one name="orderSet" class="OrderSet" not-null="true" column="order_set_id" />
		
	</class>
	
</hibernate-mapping>
